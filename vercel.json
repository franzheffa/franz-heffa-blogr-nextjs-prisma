{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/health",
      "destination": "https://agent-smith-heffa-112329442315.us-central1.run.app/health"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    { "path": "/api/cron", "schedule": "*/10 * * * *" }
  ]
}
