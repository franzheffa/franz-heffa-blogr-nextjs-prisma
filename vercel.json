{
  "env": { "BACKEND": "https://agent-smith-heffa-112329442315.us-central1.run.app" },
  "crons": [
    { "path": "/api/cron", "schedule": "*/10 * * * *" }
  ],
  "rewrites": [
    { "source": "/health", "destination": "https://agent-smith-heffa-112329442315.us-central1.run.app/health" },
    { "source": "/(?!api/.*).*", "destination": "/index.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Cache-Control", "value": "no-store" }] }
  ],
  "functions": { "api/**/*.js": { "runtime": "nodejs22.x" } }
}
