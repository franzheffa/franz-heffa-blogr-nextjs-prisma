{
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "source": "/gateway/(.*)",
      "destination": "https://agent-gateway-112329442315.europe-west1.run.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "crons": [
    {
      "path": "/api/health",
      "schedule": "0 0 * * *"
    }
  ]
}
